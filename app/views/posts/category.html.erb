<div class="row">
	<div class="span8">
		<% @posts.each do |post| %>
    <h5><%= post.title %></h5>
      <span class="date"><%= post.created_at.strftime("%B %Y") %></span>
      <p><%= post.content %></p>
    <% end %>
        
	</div>

	<div class="span4">
		<%= render :partial => 'sidebar' %>
	</div>
</div>